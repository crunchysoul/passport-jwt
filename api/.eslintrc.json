{
  "extends": [
  "eslint:recommended",
  "prettier"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"

  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "warn",
    "no-console": "off",
    "strict": ["error", "global"],
    "curly": "warn",
    "no-unused-vars" : 0,
    "no-undef" : 0,
    "no-extra-boolean-cast":0,
    "avoidEscape": true
  }
}
